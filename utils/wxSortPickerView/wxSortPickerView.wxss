.wxSortPickerViewTemTags{
  position: fixed;
  top: 10px;
  right: 0;
  background-color: #fff;
  color: cornflowerblue;
}
.wxSortPickerViewTemTag{
  text-align: center;
  padding: 3px;
  font-size: 12px;
}
.wxSortPickerViewFixedTag{
  position: fixed;
  width: 100%;
}

.wxSortPickerViewTag{
  background-color: #efeff4;
  font-size: 12px;
  font-weight: bold;
  color: #000;
  height: 24px;
  line-height: 24px;
  padding: 0 5px;
}

.wxSortPickerViewItem{
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #eee;
  margin-left: 5px;
}