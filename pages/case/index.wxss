.tuijian_list {
    padding: 0.5rem 1rem;
    position: relative;
    display: block;
    background: #fff;
    overflow: hidden;
}
.tuijian_list .info {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    font-size: 12px;
    color: #b2b2b2;
    line-height: 1.2rem;
}