.mui-content {
    background-color: #efeff4;
    color: #1A1A1A;
    letter-spacing: 1.3px;
    font-family: 'Helvetica Neue',Helvetica,sans-serif;
    font-size: 17px;
    line-height: 21px;
}
.traininglist {
    background: #fff;
    padding: 0 15px;
    position: relative;
    display: block;
}
a {
    text-decoration: none;
    color: #007aff;
}
.traininglist .list_title {
    color: #000;
    font-size: 18px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: 36px;
    line-height: 36px;
    position: relative;
}
.traininglist .list_title i {
    margin-right: 5px;
    color: #ff405f;
}
.iconfont {
    font-family: "iconfont" !important;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
}
.traininglist .list_title:after {
    background: #dedede;
    position: absolute;
    height: 1px;
    left: 0px;
    right: 0px;
    bottom: 0;
    content: '';
}
.traininglist .list_pic {
    width: 100%;
    height: 5.88rem;
    display: block;
    font-size: 0;
}
image {
    width: 100%;
    height: 100%;
    border: 0;
}
.traininglist .list_infor {
    font-size: 14px;
    color: #666;
    line-height: 1.2;
    padding: 10px 0;
}
.traininglist .list_other {
    overflow: hidden;
    padding-bottom: 5px;
}
.traininglist .list_other i {
    float: left;
    color: #01a2ff;
}
i {
    font-style: normal;
}
.traininglist .list_other span {
    float: right;
    color: #999;
}
.traininglist:after {
    background: #dedede;
    position: absolute;
    height: 1px;
    left: 0;
    right: 0;
    bottom: 0;
    content: '';
}