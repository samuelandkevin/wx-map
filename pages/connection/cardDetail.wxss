.weui_tab {
    overflow: hidden;
}
.weui_tab {
    position: relative;
    height: 100%;
}
.weui_tab_bd {
    box-sizing: border-box;
    height: 100%;
    padding-bottom: 55px;
    overflow: auto;
}
.weui_panel:first-child {
    margin-top: 0;
}
.weui_panel {
    background-color: #FFFFFF;
    position: relative;
    overflow: hidden;
    margin-top: 0;
}
.weui_cells {
    margin-top: 1.17647059em;
    background-color: #FFFFFF;
    line-height: 1.41176471;
    font-size: 17px;
    overflow: hidden;
    position: relative;
}
.weui_cells:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 1px;
    border-top: 1px solid #D9D9D9;
    color: #D9D9D9;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}
.weui_cell {
    padding: 10px 15px;
    position: relative;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}
.weui_label {
    width: 5.5em;
}
.weui_label {
    display: block;
    word-wrap: break-word;
    word-break: break-all;
}
.weui_cell_primary {
    flex: 1;
}
.info {
    color: #606060;
    font-size: 14px;
}
.weui_cells:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    border-bottom: 1px solid #D9D9D9;
    color: #D9D9D9;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}
.app_pic image {
    width: 60px;
    height: 60px;
    max-width: inherit;
    border-radius: 50%;
}

.weui_media_box .weui_media_title {
    font-weight: 400;
    font-size: 17px;
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: break-word;
    word-break: break-all;
}
.weui_media_box.weui_media_appmsg .weui_media_bd {
    flex: 1;
    min-width: 0;
}
.app_dk_jb {
    -webkit-line-clamp: 2;
}
.app_dk_jb {
    color: #606060;
    font-size: 14px;
    line-height: 1.2;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
}
.weui_media_box {
    padding: 15px;
    position: relative;
}
.weui_media_box.weui_media_appmsg {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}
.wrapper {
    background-color: #f4f4f4;
    line-height: 1.6;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.h4 {
    display: block;
    -webkit-margin-before: 0;
    -webkit-margin-after: 0.3rem;
    -webkit-margin-start: 0.8rem;
    -webkit-margin-end: 2.3rem;
    font-weight: bold;
}
.weui_btn.weui_btn_mini {
    line-height: 1.9;
    font-size: 14px;
    padding: 0 .75em;
    display: inline-block;
}
.weui_btn_plain_primary {
    border: 1px solid #4290d7;
    color: #4290d7;
}
.weui_btn_mini {
    margin-right: 10px;
}
.weui_btn {
    position: relative;
    margin-left: auto;
    box-sizing: border-box;
    text-align: center;
    text-decoration: none;
    border-radius: 5px;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    overflow: hidden;
}
.weui_cell_primary {
  -webkit-box-flex: 1;
    flex: 1;
}
.vux-timeline>ul>li {
    list-style: none;
}
.timeline-item {
    position: relative;
}
li {
    display: list-item;
    text-align: -webkit-match-parent;
}
.timeline-item-head {
    width: 12px;
    height: 12px;
    left: -1px;
    top: 2px;
    background-color: #fff;
    border: 2px solid #4290d7;
}
.timeline-item-head, .timeline-item-head-first {
    position: absolute;
    content: '';
    z-index: 99;
    border-radius: 99px;
}
.timeline-item-tail {
    position: absolute;
    content: '';
    height: 100%;
    width: 2px;
    left: 5px;
    top: 10px;
    background-color: #4290d7;
}
.timeline-item-content {
    padding: 0 0 0.24rem 2rem;
}
.vux-timeline>ul>li {
    list-style: none;
}
.vux-timeline .h4 {
    color: #606060;
    font-weight: 400;
    font-size: 16px;
}
.vux-timeline p {
    color: #606060;
    font-size: 12px;
}
p {
    display: block;
}